entity:
    id: Address
    name: "Address"
    description: "The address of a customer."
    attributes:
      - id: CustomerID
        name: "CustomerID"
        datatype: "Integer"
      - id: Street
        name: "Street"
        datatype: "Text"
      - id: CountryCode
        name: "CountryCode"
        datatype: "Text"
        customProperties:
          - id: author
            name: "Author"
            value: "CrossBreeze"
          - id: exampleEntityAttribute
            name: "ExampleEntityAttribute"
    identifiers:
      - id: Primary_Identifier
        name: "Primary Identifier"
        primary: true
        attributes:
          - CustomerID
    customProperties:
      - id: author
        name: "Author"
        value: "CrossBreeze"
      - id: exampleEntity
        name: "ExampleEntity"