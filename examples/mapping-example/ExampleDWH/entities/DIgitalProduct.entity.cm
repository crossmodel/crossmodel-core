entity:
    id: DigitalProduct
    name: "DigitalProduct"
    inherits:
      - Product
    attributes:
      - id: Attribute
        name: "ProductID"
        datatype: "Guid"
      - id: SizeInKB
        name: "SizeInKB"
        datatype: "Decimal"
      - id: DownloadURL
        name: "DownloadURL"
        datatype: "Text"
    identifiers:
      - id: Primary_Identifier
        name: "Primary Identifier"
        primary: true
        attributes:
          - Attribute